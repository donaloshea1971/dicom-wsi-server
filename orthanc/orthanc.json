{
  "Name": "Orthanc DICOM Server",
  "HttpServerEnabled": true,
  "HttpPort": 8042,
  "HttpThreadsCount": 32,
  "HttpTimeout": 120,
  "HttpRequestTimeout": 120,
  
  "DicomServerEnabled": true,
  "DicomAet": "ORTHANC",
  "DicomPort": 4242,
  "DicomCheckCalledAet": false,
  
  "RemoteAccessAllowed": true,
  "AuthenticationEnabled": true,
  "RegisteredUsers": {
    "admin": "orthanc"
  },
  
  "StorageDirectory": "/var/lib/orthanc/db",
  "IndexDirectory": "/var/lib/orthanc/db",
  
  "Plugins": [
    "/usr/share/orthanc/plugins",
    "/usr/local/share/orthanc/plugins"
  ],
  
  "PostgreSQL": {
    "EnableIndex": true,
    "EnableStorage": false,
    "Host": "postgres",
    "Port": 5432,
    "Database": "orthanc",
    "Username": "orthanc",
    "Password": "orthanc_secret",
    "MaximumConnectionRetries": 10,
    "ConnectionRetryInterval": 5,
    "Lock": false
  },
  
  "DicomWeb": {
    "Enable": true,
    "Root": "/dicom-web/",
    "EnableWado": true,
    "WadoRoot": "/wado",
    "Ssl": false,
    "QidoCaseSensitive": false,
    "Host": "0.0.0.0",
    "StudiesMetadata": "Full",
    "SeriesMetadata": "Full"
  },
  
  "DicomAlwaysAllowEcho": true,
  "DicomAlwaysAllowStore": true,
  "StableAge": 3,
  "StrictAetComparison": false,
  "StoreMD5ForAttachments": false,
  "LimitFindResults": 0,
  "LimitFindInstances": 0,
  "LogExportedResources": false,
  "KeepAlive": true,
  "TcpNoDelay": true,
  "StoreDicom": true,
  "DicomScpTimeout": 600,
  "DicomScuTimeout": 600,
  
  "HttpVerbose": false,
  "HttpDescribeErrors": false,
  "Verbose": false,
  "TraceEnabled": false,
  
  "ConcurrentJobs": 8,
  "JobsHistorySize": 50,
  
  "MediaArchiveSize": 1,
  "StorageAccessOnFind": "Never",
  "SaveJobs": false,
  
  "DefaultEncoding": "Latin1",
  "AcceptedTransferSyntaxes": [ "1.2.*" ],
  "UnknownSopClassAccepted": true,
  "DicomModalitiesInDatabase": false,
  "OrthancPeersInDatabase": false,
  
  "OverwriteInstances": true,
  
  "MaximumStorageSize": 0,
  "MaximumPatientCount": 0,
  
  "DicomCacheSize": 512,
  
  "ExecuteLuaEnabled": false,
  "LuaScripts": [],
  
  "WSI": {
    "Enable": true,
    "EnablePyramid": true,
    "EnableDicomizer": true,
    "TileWidth": 256,
    "TileHeight": 256,
    "Threads": 8,
    "CacheSize": 10000,
    "EnableConcatenations": true,
    "ForcePyramidCreation": false
  }
}